package com.amlogic.pmt.music;

import android.graphics.Bitmap;

public class MusicItemInfo {
	String mtitle    = null;;
	String malbum    = null;;
	String martist   = null;;
	Bitmap mBtp      = null;;
	
	public void setTitle(String data)
	{
		mtitle = data;
	}
	
	public void setAlbum(String data)
	{
		malbum = data;
	}
	
	public void setArtist(String data)
	{
		martist = data;
	}
	
	public void setBitmap(Bitmap data)
	{
		mBtp = data;
	}
	public String getTitle()
	{
		return mtitle;
	}
	
	public String getAlbum()
	{
		return malbum;
	}
	
	public String getartist()
	{
		return martist;
	}
	
	public Bitmap getBitmap()
	{
		return mBtp;
	}
	
}